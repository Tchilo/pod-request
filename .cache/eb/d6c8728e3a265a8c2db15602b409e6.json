weight: 400;\n  // background:no-repeat url(../assets/mobile/image-host.jpg) $dark;\n  // background: linear-gradient(rgba(18, 23, 37, 0.8) 100%, rgba(18, 23, 37, 0.8)100%), url(../assets/mobile/image-host.jpg);\n  // background-repeat: no-repeat;\n  // background-size: cover;\n  \n\n\n  @include md {\n    background:no-repeat url(../assets/tablet/image-host.jpg),$dark;\n    background-position: right;\n    display: grid;\n    background-position-x: right;\n    grid-template-rows: 28% 1fr;\n    background-position-y: 0;\n  }\n\n  @include xl {\n    background: no-repeat url(../assets/desktop/image-host.jpg), $dark;\n    background-position-x: right;\n    background-position-y: center;\n    background-size: 60%;\n\n  }\n\n}\n\nheader {\n  // background-color: $dark;\n\n  nav {\n    display: flex;\n    justify-content: center;\n    margin: 62px auto;\n  }\n\n  @include md {\n    grid-row: span 1/2;\n    justify-self: left;\n    background-color: $dark;\n\n\n    nav {\n      padding: 50px 0 0 39px;\n      margin: 0;\n\n    }\n  }\n\n  @include xl {\n    nav {\n      padding: 50px 0 0 169px;\n\n    }\n  }\n}\n\nmain {\n  padding: 0 24px;\n\n  .container {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n\n  .hero {\n    text-align: center;\n\n    h1 {\n      width: 103%;\n      font-size: 27px;\n      color: #fff;\n      line-height: 38px;\n      text-transform: uppercase;\n      margin-bottom: 16px;\n\n      & span {\n        color: $accent;\n      }\n    }\n\n    p {\n      color: $semi-white;\n      font-size: 15px;\n      line-height: 25px;\n      margin-bottom: 33px;\n    }\n  }\n\n  .icons {\n    margin-bottom: 48px;\n    display: flex;\n    gap: 20px;\n    align-items: center;\n    justify-content: space-between;\n\n    & img {\n      width: 100%;\n    }\n  }\n\n  form {\n    display: flex;\n    flex-direction: column;\n\n    .message {\n      color: $accent;\n      text-align: center;\n      margin-bottom: 10px;\n    }\n\n    .error {\n      color: rgb(230, 104, 104);\n      text-align: center;\n      margin-bottom: 10px;\n\n    }\n    input {\n      background-color: $secondary;\n      height: 46px;\n      border-radius: 28px;\n      color: #fff;\n      padding: 9px 109px 9px 32px;\n      border: none;\n      margin-bottom: 16px;\n\n      &:focus {\n        border-color: purple;\n      }\n    }\n\n    button {\n      border-radius: 28px;\n      height: 46px;\n      border: none;\n      padding: 9px 109px;\n      background: $accent;\n    }\n  }\n\n  @include md {\n    background-color: $dark;\n    margin: 0px 100px 0px 47px;\n    padding: 100px 0;\n\n    .container {\n      align-items: flex-start;\n    }\n\n    .hero {\n      text-align: left;\n\n      & h1 {\n        font-size: 48px;\n        line-height: 56px;\n        max-width: 600px;\n\n      }\n\n      & p {\n        font-size: 18px;\n        margin: 40px 0 41px;\n        max-width: 451px;\n      }\n    }\n\n    .icons {\n      order: 1;\n      margin-top: 64px;\n    }\n\n    form {\n\n      input {\n        width: 100%;\n        font-weight: 900;\n      }\n      button {\n        position: relative;\n        top: -71px;\n        width: 50%;\n        align-items: center;\n        padding: 0;\n        left: 295px;\n        font-weight: 900;\n      }\n    }\n\n \n  }\n\n  @include xl {\n    margin: 0px 689px 0px 169px;\n\n    .hero {\n      h1 {\n        font-size: 52px;\n        max-width: 665px;\n      }\n    }\n  }\n}\n\nfooter {\n  @include md {\n  background: no-repeat url(../assets/desktop/bg-pattern-dots.svg);\n  background-position-x: 30px;\n  height: 104px;\n  }\n\n  @include xl {\n    background-position-x: right;\n    position: relative;\n    top: -180px;\n  }\n}","scss/_reset.scss":"html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  font-size: 100%;\n  font: inherit;\n  vertical-align: baseline; \n  box-sizing: border-box;\n}\n\n/* HTML5 display-role reset for older browsers */\n\narticle, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {\n  display: block; }\n\nbody {\n  line-height: 1; }\n\nol, ul {\n  list-style: none; }\n\nblockquote, q {\n  quotes: none; }\n\nblockquote {\n  &:before, &:after {\n    content: '';\n    content: none; } }\n\nq {\n  &:before, &:after {\n    content: '';\n    content: none; } }\n\ntable {\n  border-collapse: collapse;\n  border-spacing: 0; }","scss/_colors.scss":"$accent: #54E6AF;\n$secondary: #2C344B;\n$dark: #121725;\n$semi-gray: #5A668A;\n$semi-white: #C2CBE5;","scss/breakpoints/_breakpoints.scss":"$breakpoints: (\n  xs: 0,\n  sm: 576px,\n  //tablet\n  md: 768px,\n  lg: 992px,\n  //desktop\n  xl: 1440px,\n);\n\n@mixin xs {\n  @media (min-width: map-get($breakpoints, \"xs\")){\n    @content;\n  }\n}\n\n@mixin sm {\n  @media (min-width: map-get($breakpoints, \"sm\")){\n    @content;\n  }\n}\n\n@mixin md {\n  @media (min-width: map-get($breakpoints, \"md\")){\n    @content;\n  }\n}\n\n@mixin lg {\n  @media (min-width: map-get($breakpoints, \"lg\")){\n    @content;\n  }\n}\n\n@mixin xl {\n  @media (min-width: map-get($breakpoints, \"xl\")){\n    @content;\n  }\n}\n\n// If breakpoint is not defined, use default breakpoint\n\n@mixin breakpoint($bp: 0) {\n  @media (min-width: $bp) {\n    @content;\n  }\n}"},"lineCount":null}},"error":null,"hash":"563776eb107761c3c049a20dd564842e","cacheData":{"env":{}}}